
@{
    Layout = null;
}

<!DOCTYPE html>
<html lang="tr">
<head>
    <title>404</title>
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <meta charset="UTF-8" />
    <link rel="stylesheet" href="~/_weberror/css/style.css" type="text/css" media="all" />
    <link href="~/_weberror/css/font-awesome.css" rel="stylesheet">
</head>
<body>
    <section class="w3l-error-61-404">
        <div class="error-61-mian">
            <div class="wrapper">
                <div class="errors-16-top">
                    <div class="errors-16-info">
                        <h3>404 </h3>
                        <p>Oops! Sayfa Bulunamadı!</p>
                        <form action="#" method="post" class="d-flex error-page-form">
                            <input type="search" placeholder="Ne aramıştınız?" required="required">
                            <button type="submit"><span class="fa fa-search" aria-hidden="true"></span></button>
                        </form>
                        <div class="social-coming-icons">
                            <a href="#" title="Facebook" class="footer-fb">
                                <span class="fa fa-facebook"
                                      aria-hidden="true"></span>
                            </a>
                            <a href="#" title="Twitter" class="footer-tw">
                                <span class="fa fa-twitter"
                                      aria-hidden="true"></span>
                            </a>
                            <a href="#" title="Google Plus" class="footer-gg">
                                <span class="fa fa-google-plus"
                                      aria-hidden="true"></span>
                            </a>
                            <a href="#" title="Linkedin" class="footer-lin">
                                <span class="fa fa-linkedin"
                                      aria-hidden="true"></span>
                            </a>
                        </div>
                    </div>
                </div>
                <div class="copyright-footer">
                    <div class="copy-right">
                        <p>
                            © @DateTime.Now.Year MyBlog. All rights reserved
                        </p>
                    </div>
                </div>
            </div>
        </div>
    </section>
</body>
</html>