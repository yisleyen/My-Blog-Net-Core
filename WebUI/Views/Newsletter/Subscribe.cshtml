<div class="tech-btm">
    <p>Güncel yazılarımız e-posta adresinizde</p>
    <form method="post">
        <input type="email" placeholder="E-Posta adresiniz" required="" name="Email" id="Email">
        <input type="submit" id="Subscrible" value="Abone Ol">
    </form>
</div>

<script type="text/javascript">
    $("#Subscrible").click(function(){
        let values = {
            Email: $("#Email").val()
        };

        $.ajax({
            type: "post",
            url : "/Newsletter/Subscribe/",
            data : values,
            success : function(func){
                let result = jQuery.parseJSON(func);
            }
        });
    });
</script>

<script src="https://code.jquery.com/jquery-3.5.1.min.js" type="text/javascript"></script>