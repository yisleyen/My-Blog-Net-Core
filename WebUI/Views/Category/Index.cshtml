@using Entity.Concrete
@model List<Category>

@{
    Layout = null;
}

<link href="~/lib/bootstrap/dist/css/bootstrap.css" rel="stylesheet" />

<table class="table table-bordered">
    <tr>
        <th>ID</th>
        <th>Name</th>
        <th>CreatedDate</th>
        <th>Status</th>
        <th></th>
    </tr>
    @foreach (var item in Model)
    {
        <tr>
            <td>@item.Id</td>
            <td>@item.Name</td>
            <td>@item.CreatedDate</td>
            <td>@item.Status</td>
            <td><a href="" class="btn btn-warning">Detay</a><a href="" class="btn btn-danger">Sil</a></td>
        </tr>
    }
</table>